<Window x:Class="QuestDashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuestDashboard"
        xmlns:vm="clr-namespace:QuestDashboard.ViewModels"
        mc:Ignorable="d"
        d:DataContext="{d:DesignInstance Type=vm:MainViewModel}"
        Title="Swoq Replay Viewer" Height="800" Width="1000"
        Closed="Window_Closed">
    <local:ReplayView DataContext="{Binding Replay}"/>
</Window>

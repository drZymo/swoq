{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "SwoqDatabase": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "Swoq",
    "UsersCollectionName": "Users"
  },
  "ReplayStorage": {
    "TrainingFolder": "%LOCALAPPDATA%\\Swoq\\Replays\\Training\\",
    "QuestFolder": "%LOCALAPPDATA%\\Swoq\\Replays\\Quest\\"
  }
}
